{
  "expo": {
    "name": "6 7",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sixsevenapp",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anshulsonidev.sixsevenapp",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleTypeRole": "Editor",
            "CFBundleURLName": "Google",
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.835051220341-ue1ognnm5sa7u3npi2gokm0fstf21ard"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/icon.png",
        "backgroundImage": "./assets/images/icon.png",
        "monochromeImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anshulsonidev.sixsevenapp",
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "com.anshulsonidev.sixsevenapp"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splashlogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      "expo-web-browser",
      "@react-native-google-signin/google-signin"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "GOOGLE_WEB_CLIENT_ID": "835051220341-681sotsfbku3vi0n4r23o4cfbvgpp8o5.apps.googleusercontent.com",
      "GOOGLE_IOS_CLIENT_ID": "835051220341-ue1ognnm5sa7u3npi2gokm0fstf21ard.apps.googleusercontent.com",
      "BACKEND_URL": "http://192.168.1.8:3000",
      "router": {},
      "eas": {
        "projectId": "c9a7bd8f-1acb-4c65-8051-39007e62322f"
      }
    },
    "owner": "anshulsonidev"
  }
}